export EDITOR='nvim'
export PATH="$HOME/.cargo/bin:$PATH"
export TERM="xterm-256color"

# Ruby installation
export PATH="/usr/local/opt/ruby/bin:$PATH"
export LDFLAGS="-L/usr/local/Cellar/ruby/2.6.3/lib"
export CPPFLAGS="-I/usr/local/Cellar/ruby/2.6.3/include"
export PKG_CONFIG_PATH="/usr/local/Cellar/ruby/2.6.3/lib/pkgconfig"
export PATH="$PATH:/usr/local/lib/ruby/gems/2.6.0/bin"

#Spark
export SPARK_HOME=/usr/local/spark
export PATH=$SPARK_HOME/bin:$PATH

#Python Exercise Script
export PYTHON_UTILS="$HOME/practise_repo/init"
export PATH=$PATH:$PYTHON_UTILS

export PATH="/opt/anaconda3/bin:$PATH"

#Flutter path
export PATH="$PATH:$HOME/flutter/bin"

# Go Installation
export GOPATH=$HOME/golang
export GOROOT=/usr/local/opt/go/libexec
export PATH=$PATH:$GOPATH/bin
export PATH=$PATH:$GOROOT/bin
